{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/no-extraneous-dependencies": "off",
        "class-methods-use-this":"off",
        "eqeqeq":"off",
        "prefer-destructuring": "off",
        "max-len":"off",
        "no-console":"off",
        "no-param-reassign": ["error", { "props": false }],
        "no-plusplus": "off"

    }
}
